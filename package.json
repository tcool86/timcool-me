    "jsonp-body": "^1.0.0",
    "karma": "^1.7.1",
    "mocha": "^4.1.0",
    "node-sass": "^4.13.0",
    "orm": "^4.0.2",
    "pg": "6.x",
    "phantomjs-prebuilt": "^2.1.16",
    "serialize-javascript": "^1.4.0",
    "sinon": "^4.1.3",
    "sinon-chai": "^2.14.0",
    "truncate": "^2.0.0",
    "uglify-js": "^3.3.4",
    "vue": "^2.5.16",
    "vue-clipboard2": "0.0.8",
    "vue-lazyload": "^1.1.4",
    "vue-media": "^1.1.1",
    "vue-mq": "^0.2.1",
